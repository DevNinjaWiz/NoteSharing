{
  "name": "api-gateway",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/services/api-gateway/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/packages/services/api-gateway",
        "main": "packages/services/api-gateway/src/main.ts",
        "tsConfig": "packages/services/api-gateway/tsconfig.app.json",
        "assets": ["packages/services/api-gateway/src/assets"],
        "webpackConfig": "packages/services/api-gateway/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "api-gateway:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "api-gateway:build:development"
        },
        "production": {
          "buildTarget": "api-gateway:build:production"
        }
      }
    }
  }
}
